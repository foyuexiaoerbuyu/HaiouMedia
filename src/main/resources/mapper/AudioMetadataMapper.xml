<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.media.haiou.mapper.AudioMetadataMapper">

    <resultMap id="BaseResultMap" type="com.media.haiou.domain.AudioMetadata">
            <id property="id" column="id" />
            <result property="mediaId" column="media_id" />
            <result property="artist" column="artist" />
            <result property="album" column="album" />
            <result property="trackNumber" column="track_number" />
            <result property="genre" column="genre" />
            <result property="lyrics" column="lyrics" />
    </resultMap>

    <sql id="Base_Column_List">
        id,media_id,artist,album,track_number,genre,
        lyrics
    </sql>
</mapper>
